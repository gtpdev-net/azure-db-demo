{
  "CosmosDb": {
    "AccountEndpoint": "https://your-account.documents.azure.com:443/",
    "DatabaseName": "your-database-name",
    "ContainerName": "your-container-name",
    "PartitionKeyPath": "/id",
    
    "ConnectionMethod": "TestAll",
    
    "ManagedIdentity": {
      "Enabled": true
    },
    
    "ConnectionString": {
      "Enabled": true,
      "Value": "AccountEndpoint=https://your-account.documents.azure.com:443/;AccountKey=your-account-key;"
    },
    
    "AccountKey": {
      "Enabled": true,
      "Value": "your-account-key"
    },
    
    "Emulator": {
      "Enabled": true
    }
  }
}
